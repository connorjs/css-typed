{
	"name": "css-typed",
	"version": "0.2.0",
	"description": "Basic TypeScript declaration generator for CSS files",
	"keywords": [
		"CSS",
		"modules",
		"types",
		"TypeScript"
	],
	"homepage": "https://github.com/connorjs/css-typed#readme",
	"bugs": {
		"url": "https://github.com/connorjs/css-typed/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Connor Sullivan",
		"email": "dev.connorjs@gmail.com",
		"url": "https://github.com/connorjs"
	},
	"files": [
		"main.js"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/connorjs/css-typed.git"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"bin": {
		"css-typed": "main.js"
	},
	"scripts": {
		"format": "prettier -l '**/*.{js,json,md}' --ignore-path .gitignore",
		"format:fix": "npm run format -- -w",
		"prepare": "is-ci || husky install",
		"test": "echo 'Error: no test specified' && exit 1"
	},
	"lint-staged": {
		"*.{js,json,md}": "prettier -w"
	},
	"engineStrict": true,
	"dependencies": {
		"css-tree": "^2.3.1",
		"glob": "^10.3.10"
	},
	"devDependencies": {
		"@types/css-tree": "^2.3.4",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1"
	}
}
